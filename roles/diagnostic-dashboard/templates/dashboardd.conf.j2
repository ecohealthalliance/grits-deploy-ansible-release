[program:dashboard]
command=node {{ diagnostic_dashboard_prefix }}/bundle/main.js
environment=METEOR_DB_NAME="{{ meteor_db_name }}",MONGO_URL="{{ meteor_mongo }}",PORT="{{ meteor_port }}",ROOT_URL="{{ public_url }}",MAIL_URL="{{ mail_url }}",GRID_URL="{{ grid_url }}"
directory={{ diagnostic_dashboard_prefix }}
user={{ grits_user }}
numprocs=1
autostart=true
autorestart=true
startsecs=10
logfile_maxbytes=150MB
logfile_backups=10
loglevel=info
